{
  "name": "@vertexvis/vertex-api-client",
  "version": "0.0.6",
  "description": "The Vertex platform API client for TypeScript and JavaScript.",
  "license": "MIT",
  "author": "Vertex Developers <support@vertexvis.com> (https://developer.vertexvis.com)",
  "homepage": "https://github.com/Vertexvis/vertex-api-client-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vertexvis/vertex-api-client-ts.git"
  },
  "bugs": {
    "url": "https://github.com/Vertexvis/vertex-api-client-ts/issues"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "dist/",
    "!dist/**/__tests__",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc --outDir dist/",
    "clean": "rm -rf dist/",
    "clean-build": "yarn clean && yarn build",
    "format": "prettier --write './**/*.+(js|jsx|ts|tsx|json|yml|yaml|md|mdx|html|css)'",
    "generate": "yarn clean && ./scripts/generate.sh && yarn format",
    "test": "jest",
    "verify": "yarn clean-build && yarn test && node dist/verify.js",
    "watch": "jest --watch"
  },
  "dependencies": {
    "axios": "^0.20.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "yarn format"
    ]
  }
}
